  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "project",
      "pageBundle": false,
      "fields": [
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "locale",
          "name": "locale",
          "type": "choice",
          "choices": [
            "en",
            "fr"
          ]
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Status",
          "name": "status",
          "type": "choice",
          "choices": [
            "idea",
            "in-progress",
            "on-hold",
            "completed",
            "cancelled"
          ]
        },
        {
          "title": "Progress",
          "name": "progress",
          "type": "choice",
          "choices": [
            "10%",
            "30%",
            "50%",
            "70%",
            "90%",
            "100%"
          ]
        },
        {
          "title": "Preview image",
          "name": "preview",
          "type": "fields",
          "fields": [
            {
              "title": "Content preview",
              "name": "url",
              "type": "image"
            },
            {
              "title": "Content preview alt",
              "name": "alt",
              "type": "string"
            }
          ]
        },
        {
          "title": "Author",
          "name": "author",
          "type": "dataFile",
          "dataFileId": "authors",
          "dataFileValue": "name",
          "dataFileKey": "name",
          "multiple": false
        },
        {
          "title": "Hreflang (Fr)",
          "name": "langFr",
          "type": "string"
        },
        {
          "title": "Hreflang (En)",
          "name": "langEn",
          "type": "string"
        },
        {
          "title": "Type of publication",
          "name": "type",
          "type": "string"
        },
        {
          "title": "Last modification date",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "projects_type",
          "name": "projects_type",
          "type": "string"
        }
      ]
    },
