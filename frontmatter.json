  "frontMatter.data.types": [
    {
      "id": "projects-types",
      "schema": {
        "title": "Projects type",
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          }
        }
      }
    }
  ],
  "frontMatter.data.files": [
    {
      "title": "All sponsors",
      "id": "all-projects-type",
      "file": "[[workspace]]/data/projects_type",
      "labelField": "name",
      "type": "projects-types"
    },
    {
      "id": "social",
      "title": "Social Medias",
      "file": "[[workspace]]/data/social.json",
      "fileType": "json",
      "labelField": "label",
      "schema": {
        "title": "Social Medias",
        "type": "object",
        "required": [
          "label",
          "link"
        ],
        "properties": {
          "label": {
            "type": "string",
            "title": "Name"
          },
          "link": {
            "type": "string",
            "title": "URL"
          }
        }
      }
    },
    {
      "id": "categories",
      "title": "Categories",
      "file": "[[workspace]]/data/categories.json",
      "fileType": "json",
      "labelField": "name",
      "schema": {
        "title": "Category",
        "type": "object",
        "required": [
          "name",
          "description"
        ],
