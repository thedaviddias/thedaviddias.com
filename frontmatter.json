      "labelField": "name",
      "schema": {
        "title": "Tag",
        "type": "object",
        "required": [
          "name",
          "description"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "logo": {
            "type": "string",
            "title": "Logo path"
          }
        }
      }
    },
    {
      "id": "authors",
      "title": "Authors",
      "file": "[[workspace]]/data/authors.json",
      "fileType": "json",
      "labelField": "name",
      "schema": {
        "title": "Author",
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "slug": {
            "type": "string",
            "title": "Slug"
          }
        }
      }
    },
    {
      "id": "uses",
      "title": "Uses",
      "file": "[[workspace]]/data/uses.json",
      "fileType": "json",
      "labelField": "title",
      "schema": {
        "title": "Title",
        "type": "object",
        "required": [
          "title",
          "url",
          "description_en",
          "category_en",
          "image"
        ],
        "properties": {
          "title": {
            "type": "string",
            "title": "Title"
          },
          "url": {
            "type": "string",
            "title": "Url"
          },
          "description_en": {
            "type": "string",
