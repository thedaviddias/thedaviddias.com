{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    }
  ],
  "frontMatter.telemetry.disable": false,
  "frontMatter.panel.freeform": false,
  "frontMatter.templates.enabled": true,
  "frontMatter.content.autoUpdateDate": false,
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.startCommand": "pnpm dev",
  "frontMatter.preview.host": "http://localhost:3000/",
  "frontMatter.taxonomy.tags": [
    "accessibilite",
    "astuces",
    "aws-amplify",
    "best-practices",
    "front-end-checklist",
    "github",
    "github-actions",
    "life",
    "life-lessons",
    "meditation",
    "morning-routines",
    "netlify",
    "nextjs",
    "open-source",
    "performance",
    "psychology",
    "react",
    "self-improvement",
    "two-factor-authentication",
    "ux"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "blog-post",
      "pageBundle": false,
      "fields": [
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "locale",
          "name": "locale",
          "type": "choice",
          "choices": [
            "en",
            "fr"
          ]
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Author",
          "name": "author",
          "type": "choice",
          "choices": [
            "David Dias"
          ]
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modification date",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Previously published platform",
          "name": "publishedOn",
          "type": "string"
        },
        {
          "title": "Previously published URL",
          "name": "publishedUrl",
          "type": "string"
        },
        {
          "title": "canonical",
          "name": "canonical",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.categories": [
    "coding",
    "expatriation",
    "productivity",
    "reflexion",
    "security",
    "side-project"
  ],
  "frontMatter.data.files": [
    {
      "id": "social",
      "title": "Social Medias",
      "file": "[[workspace]]/data/social.json",
      "fileType": "json",
      "labelField": "label",
      "schema": {
        "title": "Social Medias",
        "type": "object",
        "required": [
          "label",
          "link"
        ],
        "properties": {
          "label": {
            "type": "string",
            "title": "Name"
          },
          "link": {
            "type": "string",
            "title": "URL"
          }
        }
      }
    }
  ]
}